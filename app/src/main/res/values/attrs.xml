<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--声明样式-->
    <declare-styleable name="InputView">
        <!-- 输入框前的图标-->
        <attr name="input_icon" format="reference"></attr>
        <!-- 输入框内容提示-->
        <attr name="input_hint" format="string"></attr>
        <!-- 是否以密文展示-->
        <attr name="is_password" format="boolean"></attr>
    </declare-styleable>

</resources>